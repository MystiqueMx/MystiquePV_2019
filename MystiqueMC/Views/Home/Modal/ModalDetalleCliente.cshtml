
<div class="modal fade" id="modal-detalle-cliente" tabindex="-1" role="dialog">
    <br /><br /><br /><br /><br /><br /><br /><br /><br />
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <div class="row">
                    <div class="col-md-4">
                        <h3 class="modal-title"><span>Beneficios Solicitados</span></h3>
                    </div>
                    <div class="col-md-7">
                        <div class="input-group">
                            <span class="input-group-addon" style="color:#000; background-color:transparent; border-color:transparent;">
                                <span class="modal-title">
                                    <label style="color:#6C6A67">Ordenar por</label>
                                </span>
                            </span>
                            <select class="form-control input-sm" id="sel1" aria-describedby="basic-addon1" name="SearchEstatus">
                                <option selected="selected" value="">-Seleccione-</option>
                                <option value="true">Activo</option>
                                <option value="false">Inactivo</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-1">
                        <button type="button" class="close pull-right" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    </div>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    @*<div class="form-group" style="display:none">
                        @*<label>cliente: </label> <span id="empleado-nombre"> </span>*@
                </div>
                <table class="table invoice-table" id="table-detalle-cliente">
                    <thead>
                        <tr>
                            <th>Nombre</th>
                            <th>Email</th>
                            <th>Teléfono</th>
                            <th>Colonia</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
